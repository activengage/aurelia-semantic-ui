    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-1": "^6.5.0",
    "del": "^2.2.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-bump": "^2.2.0",
    "gulp-debug": "^2.1.2",
    "gulp-eslint": "^3.0.1",
