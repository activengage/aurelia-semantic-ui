    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jspm": "^2.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-safari-launcher": "^1.0.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.2.2",
    "vinyl-paths": "^2.1.0",
    "yargs": "^4.8.1"
  },
  "peerDepedencies": {
    "aurelia-framework": "^1.0.0",
    "semantic-ui": "^2.2",
    "jquery": "^3.1"
